    'rs', 'DisplayName', 'H2 TDSE 810nm, r=1.50'); 
% plot(E_CC, -t_interp ./ T_AU, 'r--', 'HandleVisibility', 'off'); 
plot(E_CC, -interp_140, 'r--', 'HandleVisibility', 'off'); 
plot(E_CC, -interp_145, 'r--', 'HandleVisibility', 'off'); 
